package com.muxxu.kube.events {	import com.muxxu.kube.vo.Ad;	import flash.events.Event;	/**	 * Event fired by AdsData singleton	 * 	 * @author Francois	 */	public class AdsDataEvent extends Event {				public static const ADD_AD_COMPLETE:String		= "ADD_AD_COMPLETE";		public static const ADD_AD_ERROR:String			= "ADD_AD_ERROR";		public static const GET_ADS_COMPLETE:String		= "GET_ADS_COMPLETE";		public static const GET_ADS_ERROR:String		= "GET_ADS_ERROR";				private var _resultCode:int;		private var _ads:Vector.<Ad>;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>AdsADataEvent</code>.		 */		public function AdsDataEvent(type:String, resultCode:int = 0, ads:Vector.<Ad> = null, bubbles:Boolean = false, cancelable:Boolean = false) {			_ads = ads;			_resultCode = resultCode;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the result code.		 */		public function get resultCode():int {			return _resultCode;		}				/**		 * Gets the loaded ads.		 */		public function get ads():Vector.<Ad> { return _ads; }		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Makes a clone of the event object.		 */		override public function clone():Event {			return new AdsDataEvent(type, resultCode, ads, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */			}}