package com.muxxu.kube.events {	import flash.events.Event;	/**	 * Event fired by <code>VoteBar</code> component.	 * 	 * @author  Francois	 */	public class VoteBarEvent extends Event {				public static const VOTE:String = "vote";				private var _note:int;		private var _id:String;								/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>VoteBarEvent</code>.		 */		public function VoteBarEvent(type:String, note:int, id:String, bubbles:Boolean = false, cancelable:Boolean = false) {			_id = id;			_note = note;			super(type, bubbles,cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the note.		 */		public function get note():int { return _note; }				/**		 * Gets the item's id.		 */		public function get id():String { return _id; }		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Creates an event's clone.		 */		override public function clone():Event{			return new VoteBarEvent(type, note, id, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */	}}