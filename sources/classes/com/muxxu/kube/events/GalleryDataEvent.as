package com.muxxu.kube.events {	import flash.events.Event;	/**	 * Event fired by the <code>GalleryData</code> singleton.	 * 	 * @author  Francois	 */	public class GalleryDataEvent extends Event {		public static const ITEMS_LOADING_SUCCESS:String	= "itemsLoadingSuccess";		public static const ITEMS_LOADING_ERROR:String		= "itemsLoadingError";		public static const IMAGE_SENT:String				= "imageSent";		public static const IMAGE_SENDING_ERROR:String		= "imageSendingError";		public static const VOTE_SUBMITTED:String			= "voteSubmitSuccess";		public static const VOTE_SUBMIT_ERROR:String		= "voteSubmitError";		public static const ENLARGE_IMAGE:String			= "enlargeImage";				private var _errorCode:int;		private var _data:*;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>GalleryDataEvent</code>.		 */		public function GalleryDataEvent(type:String, errorCode:int = 0, data:* = null ,bubbles:Boolean = false, cancelable:Boolean = false) {			_data = data;			_errorCode = errorCode;			super(type, bubbles, cancelable);		}								/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the server's error code.		 */		public function get errorCode():int { return _errorCode; }				/**		 * Gets the request data.		 */		public function get data():* { return _data; }		/* ****** *		 * PUBLIC *		 * ****** */		override public function clone():Event{			return new GalleryDataEvent(type, errorCode, data, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */	}}