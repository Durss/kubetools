package com.muxxu.kube.events {	import com.muxxu.kube.vo.Point3D;	import flash.events.Event;		/**	 * Event fired by LovelConnectionManager	 * 	 * @author Francois	 */	public class LocalConnectionManagerEvent extends Event {				public static const LC_CONNECT_ERROR:String = "LC_CONNECT_ERROR";		public static const LC_SECURITY_ERROR:String = "LC_SECURITY_ERROR";				public static const ENTER_ZONE:String = "ENTER_ZONE";		public static const DOLMEN_TOUCHED:String = "DOLMEN_TOUCHED";		public static const EXACT_COORDINATES:String = "EXACT_COORDINATES";				private var _zone:Point3D;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>LocalConnectionManagerEvent</code>.		 */		public function LocalConnectionManagerEvent(type:String, zone:Point3D = null, bubbles:Boolean = false, cancelable:Boolean = false) {			_zone = zone;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the current zone the player is in.		 */		public function get zone():Point3D { return _zone; }		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Makes a clone of the event object.		 */		override public function clone():Event {			return new LocalConnectionManagerEvent(type, zone, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */			}}