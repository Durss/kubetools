package com.muxxu.kube.events {	import flash.events.Event;	/**	 * Event fired by the view through the <code>ViewLocator</code>.	 * 	 * @author  Francois	 */	public class ViewEvent extends Event {				public static const FILL_CELL:String		= "fillCell";		public static const EMPTY_CELL:String		= "emptyCell";				private var _data:*;										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>ViewEvent</code>.		 */		public function ViewEvent(type:String, data:*, bubbles:Boolean = false, cancelable:Boolean = false) {			_data = data;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the event's data.		 */		public function get data():* { return _data; }		/* ****** *		 * PUBLIC *		 * ****** */		override public function clone():Event {			return new ViewEvent(type, data, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */	}}