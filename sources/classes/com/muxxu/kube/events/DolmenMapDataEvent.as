package com.muxxu.kube.events {	import com.muxxu.kube.components.map.MapEntry;	import flash.events.Event;	import flash.geom.Rectangle;	/**	 * Event fired by the <code>DolmenData</code> singleton.	 * 	 * @author  Francois	 */	public class DolmenMapDataEvent extends Event {				public static const LOAD_DOLMENS_COMPLETE:String	= "loadDolmensComplete";		public static const LOAD_DOLMENS_ERROR:String		= "loadDolmensError";				public static const CENTER_MAP_ON:String			= "centerMapOn";				public static const DOLMEN_TOUCHED:String			= "dolmenTouched";				public static const UPDATE_MAP:String				= "updateMap";				public static const ADD_DOLMEN_COMPLETE:String		= "addDolmenComplete";		public static const ADD_DOLMEN_ERROR:String			= "addDolmenError";				private var _resultCode:int;		private var _area:Rectangle;		private var _dolmens:Vector.<MapEntry>;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>DolmenDataEvent</code>.		 */		public function DolmenMapDataEvent(type:String, resultCode:int = 0, area:Rectangle = null, dolmens:Vector.<MapEntry> = null, bubbles:Boolean = false, cancelable:Boolean = false) {			_dolmens = dolmens;			_area = area;			_resultCode = resultCode;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the result code.		 */		public function get resultCode():int {			return _resultCode;		}				/**		 * Gets the area concerned by the loading.		 */		public function get area():Rectangle {			return _area;		}				/**		 * Gets the dolmens to add.		 */		public function get dolmens():Vector.<MapEntry> {			return _dolmens;		}		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Makes a clone of the event object.		 */		override public function clone():Event {			return new DolmenMapDataEvent(type, resultCode, area, dolmens, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */	}}