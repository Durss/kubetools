package com.muxxu.kube.events {	import flash.events.Event;	/**	 * Event fired by the <code>LoadManager</code> singleton.	 * 	 * @author  Francois Dursus	 */	public class LoadManagerEvent extends Event {				public static const START:String		= "startLoading";		public static const PROGRESS:String		= "progressLoading";		public static const COMPLETE:String		= "completeLoading";				private var _percent:Number;						/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>LoadManagerEvent</code>.		 */		public function LoadManagerEvent(type:String, percent:Number = 0, bubbles:Boolean = false, cancelable:Boolean = false) {			_percent = percent;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */				/**		 * Gets the loaded percent.		 */		public function get percent():Number { return _percent; }		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}