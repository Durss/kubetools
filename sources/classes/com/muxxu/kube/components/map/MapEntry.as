package com.muxxu.kube.components.map {	/**	 * Contains the data about a map's point.	 * 	 * @author  Francois	 */	public class MapEntry {		private var _x:int;		private var _y:int;		private var _icon:IZoomableMapItem;		private var _rawData:XML;								/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>MapEntry</code>.		 */		public function MapEntry(x:int, y:int, icon:IZoomableMapItem, rawData:XML) {			_rawData = rawData;			_x = x;			_y = y;			_icon = icon;		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the X coordinate.		 */		public function get x():int { return _x; }				/**		 * Gets the Y coordinate.		 */		public function get y():int { return _y; }				/**		 * Gets the icon to display on the map.		 */		public function get icon():IZoomableMapItem { return _icon; }				/**		 * Sets the icon to display on the map.		 */		public function set icon(value:IZoomableMapItem):void { _icon = value; }				/**		 * Gets the raw XML data.		 */		public function get rawData():XML { return _rawData; }				/**		 * Sets the raw XML data.		 */		public function set rawData(value:XML):void { _rawData = value; }				/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Gets a String representation of the object.		 */		public function toString():String {			return "[MapEntry :: x="+x+", y="+y+"]";		}						/* ******* *		 * PRIVATE *		 * ******* */	}}