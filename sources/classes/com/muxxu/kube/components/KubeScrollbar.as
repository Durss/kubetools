package com.muxxu.kube.components {	import com.muxxu.kube.graphics.scrollbar.ScrollbarDownBtSkin;	import com.muxxu.kube.graphics.scrollbar.ScrollbarScrollerBtSkin;	import com.muxxu.kube.graphics.scrollbar.ScrollbarTrackBtSkin;	import com.muxxu.kube.graphics.scrollbar.ScrollbarUpBtSkin;	import com.nurun.components.button.visitors.FrameVisitor;	import com.nurun.components.button.visitors.FrameVisitorOptions;	import com.nurun.components.scroll.scroller.scrollbar.Scrollbar;	import com.nurun.components.scroll.scroller.scrollbar.ScrollbarSkin;	/**	 * Creates a pre-skinned scrollbar.	 * 	 * @author  Francois	 */	public class KubeScrollbar extends Scrollbar {				private var _up:ScrollbarUpBtSkin;
		private var _down:ScrollbarDownBtSkin;
		private var _scroller:ScrollbarScrollerBtSkin;
		private var _track:ScrollbarTrackBtSkin;
										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>KubeScrollbar</code>.		 */		public function KubeScrollbar() {			var opts:FrameVisitorOptions = new FrameVisitorOptions("out", "over", "down", "out");						_up = new ScrollbarUpBtSkin();			_down = new ScrollbarDownBtSkin();			_scroller = new ScrollbarScrollerBtSkin();			_track = new ScrollbarTrackBtSkin();						var skin:ScrollbarSkin = new ScrollbarSkin(_up, _down, _scroller, null, _track);						var fv:FrameVisitor = new FrameVisitor();			fv.addTarget(_up, opts);			skin.upBt.accept(fv);						fv = new FrameVisitor();			fv.addTarget(_down, opts);			skin.downBt.accept(fv);						fv = new FrameVisitor();			fv.addTarget(_scroller, opts);			skin.scrollerBt.accept(fv);						fv = new FrameVisitor();			fv.addTarget(_track, opts);			skin.scrollTrackBt.accept(fv);						super(skin);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */						/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}