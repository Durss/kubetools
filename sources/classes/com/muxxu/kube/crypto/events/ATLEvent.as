package com.muxxu.kube.crypto.events {	import flash.events.Event;	/**	 * Event fired by the <code>ATL2</code> class.	 * 	 * @author  Francois	 */	public class ATLEvent extends Event {		private var _text:String;				public static const ENCRYPT_PROGRESS:String	= "encryptProgress";		public static const ENCRYPT_COMPLETE:String	= "encryptComplete";		public static const DECRYPT_PROGRESS:String	= "decryptProgress";		public static const DECRYPT_COMPLETE:String	= "decryptComplete";										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>ATL2Event</code>.		 */		public function ATLEvent(type:String, text:String, bubbles:Boolean = false, cancelable:Boolean = false) {			_text = text;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Contains the actual encryption state.		 */		public function get text():String { return _text; }		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * Makes a clone of the event.		 */		override public function clone():Event {			return new ATLEvent(type, text, bubbles, cancelable);		}								/* ******* *		 * PRIVATE *		 * ******* */	}}