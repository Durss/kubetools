package com.muxxu.kube.vo {		/**	 * 	 * @author Francois	 */	public class Dolmen {		private var _x:int;		private var _y:int;		private var _hidden:Boolean;								/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>Dolmen</code>.		 */		public function Dolmen(x:int, y:int, hidden:Boolean) {			_hidden = hidden;			_y = y;			_x = x;		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the X position of the dolmen;		 */		public function get x():int { return _x; }				/**		 * Gets the Y position of the dolmen.		 */		public function get y():int { return _y; }				/**		 * Gets if the dolmen should be hidden or not.		 */		public function get hidden():Boolean { return _hidden; }		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}