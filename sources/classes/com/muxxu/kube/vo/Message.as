package com.muxxu.kube.vo {		/**	 * 	 * @author  Francois	 */	public class Message {				private var _id:String;		private var _data:*;								/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>Message</code>.		 */		public function Message(id:String, data:* = null) {			_id = id;			_data = data;		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */				/**		 * Gets the message's data.		 */		public function get data():* { return _data; }				/**		 * Gets the message's ID.		 * 		 * @see MessageIds MessageIds		 */		public function get id():String { return _id; }		/* ****** *		 * PUBLIC *		 * ****** */		public function toString():String {			return "[Message :: id=\""+id+"\", data="+data+"]";		}						/* ******* *		 * PRIVATE *		 * ******* */			}}