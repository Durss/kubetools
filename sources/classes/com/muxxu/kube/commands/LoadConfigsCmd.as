package com.muxxu.kube.commands {	import com.muxxu.kube.data.SharedObjectManager;
	import com.nurun.core.commands.events.CommandEvent;	import com.nurun.structure.environnement.configuration.Config;	import flash.events.Event;	import flash.events.IOErrorEvent;	import flash.net.URLRequest;	/**	 * 	 * @author  Francois	 */	public class LoadConfigsCmd extends LoadCssCmd {		private var _url:String;										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>LoadConfigsCmd</code>.		 */		public function LoadConfigsCmd(url:String) {			_url = url;			super();		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/* ****** *		 * PUBLIC *		 * ****** */		/**		 * @inheritDoc		 */		override public function execute():void {			_loader.load(new URLRequest(_url));		}								/* ******* *		 * PRIVATE *		 * ******* */		/**		 * Called when loading completes.		 */		override protected function loadCompleteHandler(e:Event):void {			var xml:XML;			try {				xml = new XML(_loader.data);			}catch(error:Error){				dispatchEvent(new CommandEvent(CommandEvent.ERROR, "Configs XML bad formated."));				return;			}			Config.populate(xml);			//Sets the local/online mode.			if(SharedObjectManager.getInstance().localMode) {				Config.addPath("server", Config.getVariable("serverOffline"));			}else{				Config.addPath("server", Config.getVariable("serverOnline"));			}			dispatchEvent(new CommandEvent(CommandEvent.COMPLETE));		}		/**		 * Called if loading fails.		 */		override protected function loadErrorHandler(e:IOErrorEvent):void {			dispatchEvent(new CommandEvent(CommandEvent.ERROR, "Configs file not found."));		}			}}