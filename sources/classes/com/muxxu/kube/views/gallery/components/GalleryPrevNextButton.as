package com.muxxu.kube.views.gallery.components {	import com.muxxu.kube.graphics.BackPrevNextbt;	import com.muxxu.kube.graphics.GalleryNextArrow;	import com.muxxu.kube.graphics.GalleryPrevArrow;	import com.nurun.components.button.GraphicButton;	import com.nurun.components.button.IconAlign;	import com.nurun.components.button.visitors.FrameVisitor;	import com.nurun.components.button.visitors.PropertiesVisitor;	import com.nurun.components.button.visitors.PropertiesVisitorOptions;	import flash.display.MovieClip;	/**	 * 	 * @author  Francois	 */	public class GalleryPrevNextButton extends GraphicButton {										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>GalleryPreviousButton</code>.		 */		public function GalleryPrevNextButton(isNext:Boolean) {			super(new BackPrevNextbt(), isNext? new GalleryNextArrow() : new GalleryPrevArrow());						var pv:PropertiesVisitor = new PropertiesVisitor();			var opt:PropertiesVisitorOptions = new PropertiesVisitorOptions({alpha:.5}, {alpha:1});			pv.addTarget(this, opt);			accept(pv);			var fv:FrameVisitor = new FrameVisitor();			fv.addTarget(background as MovieClip);			fv.addTarget(icon as MovieClip);			accept(fv);						iconAlign = IconAlign.CENTER;			iconAlign = IconAlign.CENTER;		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */			}}